.container {
  margin:30rpx;
  padding: 30rpx;
  border: 1rpx solid rgba(214, 147, 58, 0.842);
  border-radius: 30rpx;
}

.container1{
  display: flex;
  justify-content: space-between;
}

.delete{
  border: 1rpx solid rgba(214, 147, 58, 0.842);
  border-radius: 30rpx;
  padding:20rpx;
}

.proof {
  font-size: 45rpx;
  font-weight: bolder;
}

.word {
  font-size: 30rpx;
}

.input {
  font-size: 35rpx;
  margin-top: 30rpx;
  border: 1rpx solid rgba(214, 147, 58, 0.842);
  padding:20rpx;
  border-radius:30rpx ;
}

.imgurl{
  font-size: 30rpx;
  color: rgba(214, 147, 58, 0.842);
}

.imgurl:active{
  color: rgb(56, 163, 158);
}

.attention{
  font-size: 25rpx;
}

.add {
  margin-top: 20rpx;
  padding: 20rpx;
  background-color: rgba(214, 147, 58, 0.842);
  color: white;
  text-align: center;
  border-radius: 10rpx;
}

