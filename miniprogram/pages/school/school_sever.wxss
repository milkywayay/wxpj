.container {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}


.toilet_paper,
.getParcel {
  display: flex;
  flex-direction: column; /* 垂直排列 */
  align-items: center; /* 水平居中 */
}

.toilet_paper_photo,
.getParcel_photo{
  width: 100rpx; 
  height: 100rpx;
  margin-bottom: 10rpx; 
}

.toilet_paper_word,
.getParcel_word{
  font-size: 32rpx;
  color: #333;
}

.container1{
  display: flex;
  justify-content: space-around; 
  align-items: center;
  padding: 40rpx;
}

.location_photo{
  height: 35rpx;
  width: 35rpx;
}

.address_formatted{
  color: rgb(179, 148, 83);
}
